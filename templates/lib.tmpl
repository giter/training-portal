{{define "head"}}
	<head>
		<title>{{if .HTMLTitle}}{{.HTMLTitle}} - {{end}}嘉兴房产团购网</title>
		<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
		<link rel="stylesheet" type="text/css" href="/css.css?{{.CONTEXT.UPTIME}}" />
	</head>
{{end}}

{{define "top"}}
	<div id='top'>
		<div class='wrap'>
			<ul>
				<li class='hoverable clickable'><a href='/'>首页</a></li>
				<li class='hoverable clickable {{if .HTMLComponent}}{{if eq .HTMLComponent "新房"}}active{{end}}{{end}}'><a href='/newhouse/'>新房</a></li>
				<li class='hoverable clickable'><a href='#'>二手房</a></li>
				<li class='hoverable clickable {{if .HTMLComponent}}{{if eq .HTMLComponent "资讯"}}active{{end}}{{end}}'><a href='/news/'>资讯</a></li>
				<li class='hoverable clickable'><a href='#'>装修</a></li>
				<li class='hoverable clickable {{if .HTMLComponent}}{{if eq .HTMLComponent "团购"}}active{{end}}{{end}}'><a href='/groupbuy/'>团购<i class='icon-thumb-top'></i></a></li>
			</ul>
			<div class='more'>
				<i class='icon-user-gray'>&nbsp;</i>
				<a href='#'>登录</a>
				<abbr>|</abbr>
				<a href='#'>注册</a>
			</div>
		</div>
	</div>
{{end}}

{{define "pager"}}

	{{ if gt .Page.Last 1 }}
	<ul class='pager'>
		{{if eq .Page.Previous 0}}
		{{ else }}
			<li class='{{if eq .Page.Previous 0}}disabled{{end}} hoverable clickable'><a href='{{ queryN $.Page.URL "p" .Page.Previous }}'>&lt;上一页</a></li>
		{{ end }}
		
		{{ range .Page.List }}
			{{ if eq . 0 }}
				<li class='text'>…</li>
			{{else}}
				<li class='{{if eq . $.Page.Current | not}}hoverable clickable{{end}} {{if eq . $.Page.Current}}active{{end}}'>
					{{if eq . $.Page.Current}}
						{{.}}
					{{else}}
						<a href='{{ queryN $.Page.URL "p" . }}'>{{.}}</a>
					{{end}}
				</li>
			{{ end }}
		{{ end }}
		{{if eq .Page.Next 0}}
		{{ else }}
			<li class='{{if eq .Page.Next 0}}disabled{{end}} hoverable clickable'><a href='{{ queryN $.Page.URL "p" .Page.Next }}'>下一页&gt;</a></li>
		{{ end }}
	</ul>
	{{ end }}
{{end}}

{{define "brand" }}
<div id='brand' class='clearfix'>
		
	<div class='logo'>
		<a href='/'><img src='/images/logo.png'/></a>
		<span class='f22 primary'>&nbsp;{{if .HTMLTitle}}<abbr>|</abbr> {{.HTMLTitle}}{{end}}</span>
	</div>
	
	{{ template "newhouse-search" .}}
</div>
{{end}}

{{define "newhouse-brand" }}
<div id='brand' class='clearfix'>
		
	<div class='search'>
			<h3>
				<i class='icon-phone-large'>&nbsp;</i> 
				免费咨询售楼处：
				<span class='f18'>
				{{ template "newhouse-phone".Newhouse }}
				</span>
			</h3>
	</div>
	
	<div class='logo'>
		<a href='/'><img src='/images/logo.png'/></a>
		<span class='f22'>
			&nbsp;{{if .HTMLTitle}}<abbr>|</abbr> {{.HTMLTitle}}{{end}}
			{{ if .Newhouse.Schedule }}
			{{ if .Newhouse.Schedule | len }}
				{{range .CONTEXT.KVs.Schedule.Nodes }}
					{{if in $.Newhouse.Schedule .Value}}
						<i class='icon-pop'>{{.Name}}</i>
					{{end}}
				{{end}}
			{{ end }}
			{{ end }}
		</span>
	</div>
</div>
{{end}}

{{define "newhouse-phone"}}
	{{if equals .Order "" | not }}
		<span class='primary'>{{.Order}}</span>
		{{if .OrderExtension}}
			转 <span class='primary'>{{.OrderExtension}}</span>
		{{end}}
	{{else}}
		{{if len .Phone}}
			{{range slice .Phone 0 1}}
				<span class='primary'>{{.}}</span>
			{{end}}
		{{end}}
	{{end}}
{{end}}

{{define "newhouse-search"}}
<div class='search'>
	<div class='form clearfix'>
		<form action='/estate/list.html'>
			<input class='submit' type='submit' value='搜索' />
			<input class='text' name='q' type='text' placeholder='输入楼盘名称、地址或开发商等' /> 
		</form>
	</div>
	<div class='quick-search clearfix'>
		<ul>
			<li><a href='#'>嘉兴万达</a></li>
			<li><a href='#'>中港城</a></li>
			<li><a href='#'>旭辉广场</a></li>
			<li><a href='#'>南湖区</a></li>
			<li><a href='#'>江南摩尔</a></li>
			<li><a href='#'>嘉兴</a></li>
			<li><a href='#'>桐乡</a></li>
			<li><a href='#'>嘉善</a></li>
			<li><a href='#'>海宁</a></li>
			<li><a href='#'>平湖</a></li>
		</ul>
	</div>
</div>
{{end}}

